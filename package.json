{
  "name": "3dgraph",
  "version": "0.0.1",
  "private": true,
  "author": "Momo",
  "scripts": {
    "prepare": "npm run gcp-build",
    "dev": "vite dev",
    "build": "vite build",
    "deploy": "gcloud app deploy",
    "gcp-build": "",
    "start": "node build/index.js",
    "theme": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@smui/icon-button": "^7.0.0-beta.15",
    "@smui/linear-progress": "^7.0.0-beta.15",
    "@smui/tab": "^7.0.0-beta.15",
    "@smui/tab-bar": "^7.0.0-beta.15",
    "@smui/top-app-bar": "^7.0.0-beta.15",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-node": "^1.3.1",
    "@sveltejs/kit": "^1.27.4",
    "@theatre/core": "^0.7.0",
    "@theatre/studio": "^0.7.0",
    "@threlte/core": "^6.1.1",
    "@threlte/extras": "^7.5.1",
    "@threlte/flex": "^0.1.0",
    "@threlte/theatre": "^2.1.3",
    "@types/three": "^0.157.2",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte": "^2.30.0",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^4.0.5",
    "svelte-check": "^3.6.0",
    "svelte-routing": "^2.7.0",
    "three": "^0.157.0",
    "tslib": "^2.4.1",
    "typescript": "^5.3.2",
    "vite": "^4.4.2"
  },
  "type": "module",
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@octokit/core": "^5.0.2",
    "@threlte/rapier": "^1.1.3",
    "loremipsum": "^0.1.2",
    "material-design-colors": "^1.0.2"
  }
}
